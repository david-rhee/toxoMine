      	<fieldconfig fieldExpr="toxoplasmaMutant.name" label="Toxoplasma Mutant" />
      	<fieldconfig fieldExpr="persistentDataFile.name" label="Persistent Data File" />
      	<fieldconfig fieldExpr="submissionDataFile.name" label="Submission Data File" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.GeneList">
    <fields>
      	<fieldconfig fieldExpr="name"/>
      	<fieldconfig fieldExpr="readCount"/>
      	<fieldconfig fieldExpr="gene.primaryIdentifier" label="Gene"/>
      	<fieldconfig fieldExpr="submission.name" label="Submission"/>
    </fields>
  </class>

<!-- exporters -->
  <tableExportConfig id="sequence" className="org.intermine.bio.web.export.SequenceHttpExporter"/>
  <tableExportConfig id="gff3" className="org.intermine.bio.web.export.GFF3HttpExporter"/>
  <tableExportConfig id="bed" className="org.intermine.bio.web.export.BEDHttpExporter"/>

  <reportdisplayers>    
