<?xml version="1.0"?>
<userprofile>
  <items/>
  <bags/>
  <queries>
  </queries>
<template-queries>
<template name="Organism_Protein" title="Organism --&gt; All proteins." longDescription="Show all the proteins from a particular organism." comment="">
  <query name="Organism_Protein" model="genomic" view="Protein.primaryIdentifier Protein.primaryAccession" longDescription="Show all the proteins from a particular organism." sortOrder="Protein.primaryIdentifier asc">
    <constraint path="Protein.organism.name" editable="true" description="Show all the proteins from the organism:" op="=" value="Toxoplasma gondii"/>
  </query>
</template>
<template name="Gene_Protein" title="Gene --&gt; Proteins." longDescription="Show the proteins for a particular gene." comment="06.06.07 updated to work from gene class - Philip">
  <query name="Gene_Protein" model="genomic" view="Gene.primaryIdentifier Gene.symbol Gene.proteins.primaryIdentifier Gene.proteins.primaryAccession" longDescription="Show the proteins for a particular gene." sortOrder="Gene.primaryIdentifier asc">
    <pathDescription pathString="Gene.proteins" description="Protein"/>
    <constraint path="Gene" editable="true" op="LOOKUP" value="ABRA"/>
  </query>
</template>
<template name="All_Proteins_In_Organism_To_Publications" title="All proteins in organism --&gt; Publications." longDescription="Show the publications for each gene in one organism. References are loaded from FlyBase for all Drosophila species, and from NCBI for D. melanogaster, C. elegans and S. cerevisiae." comment="">
  <query name="All_Proteins_In_Organism_To_Publications" model="genomic" view="Protein.primaryIdentifier Protein.primaryAccession Protein.organism.name Protein.publications.firstAuthor Protein.publications.title Protein.publications.year Protein.publications.journal Protein.publications.volume Protein.publications.pages Protein.publications.pubMedId" sortOrder="Protein.primaryIdentifier asc">
    <constraint path="Protein.organism.name" editable="true" description="Organism.name" op="=" value="Toxoplasma gondii"/>
  </query>
</template>

<template name="Project_Submission" title="Project --&gt; Submissions." longDescription="Show all submissions in given project." comment="">
  <query name="Project_Submission" model="genomic" view="Project.name Project.submissions.name Project.submissions.TCid Project.submissions.design Project.submissions.publicReleaseDate" longDescription="Show all submissions in given project." sortOrder="Project.name asc">
    <pathDescription pathString="Project.submissions" description="Submission"/>
    <constraint path="Project.submissions.TCid" editable="true" description="Submission.TCid" op="CONTAINS" value="TC"/>
  </query>
</template>
<template name="Gene_BindingSites" title="Gene --&gt; Binding Sites." longDescription="Show all protein binding sites overlapping given gene." comment="">
  <query name="" model="genomic" view="Gene.primaryIdentifier Gene.overlappingFeatures.primaryIdentifier Gene.overlappingFeatures.chromosome.primaryIdentifier Gene.overlappingFeatures.chromosomeLocation.end Gene.overlappingFeatures.chromosomeLocation.start Gene.overlappingFeatures.submissions.name Gene.overlappingFeatures.submissions.antibodies.name Gene.overlappingFeatures.submissions.TCid" longDescription="Show all protein binding sites overlapping given gene." sortOrder="Gene.primaryIdentifier asc" constraintLogic="A and B">
    <pathDescription pathString="Gene.overlappingFeatures" description="Binding site"/>
    <pathDescription pathString="Gene.overlappingFeatures.submissions" description="Submission"/>
    <pathDescription pathString="Gene.overlappingFeatures.submissions.antibodies" description="Antibody"/>
    <constraint path="Gene" code="A" editable="true" op="LOOKUP" value="TGME49_222410" extraValue="T. gondii"/>
    <constraint path="Gene.overlappingFeatures" type="BindingSite"/>
    <constraint path="Gene.overlappingFeatures.submissions.antibodies.name" code="B" editable="true" description="Show binding sites only for the following factor (factor being a chIP target such as a transcription factor or histone modification.):" switchable="off" op="=" value="H2A.Z_Sergio_Angel"/>
  </query>
</template>

<template name="SubmissionSubmission_overlappingBindingSites" title="Submission(s) + submission(s) --&gt; overlapping binding sites" longDescription="For two submissions or lists of submissions show overlapping binding sites.  Note:  overlapping does not mean a 100% overlap, the overlap can be complete or partial at either end." comment="">
  <query name="" model="genomic" view="Submission.name Submission.TCid Submission.features.primaryIdentifier Submission.features.chromosome.primaryIdentifier Submission.features.chromosomeLocation.end Submission.features.chromosomeLocation.start Submission.features.overlappingFeatures.primaryIdentifier Submission.features.overlappingFeatures.chromosome.primaryIdentifier Submission.features.overlappingFeatures.chromosomeLocation.end Submission.features.overlappingFeatures.chromosomeLocation.start Submission.features.overlappingFeatures.submissions.name Submission.features.overlappingFeatures.submissions.TCid" longDescription="For two submissions or lists of submissions show overlapping binding sites.  Note:  overlapping does not mean a 100% overlap, the overlap can be complete or partial at either end." sortOrder="Submission.name asc" constraintLogic="A and B">
    <join path="Submission.features.chromosome" style="OUTER"/>
    <join path="Submission.features.chromosomeLocation" style="OUTER"/>
    <join path="Submission.features.overlappingFeatures.chromosome" style="OUTER"/>
    <join path="Submission.features.overlappingFeatures.chromosomeLocation" style="OUTER"/>
    <constraint path="Submission" code="A" editable="true" description="Show binding sites from submission(s):" op="LOOKUP" value="TC1_CS" extraValue="T. gondii"/>
    <constraint path="Submission.features.overlappingFeatures.submissions" code="B" editable="true" description="that overlap binding sites from submission(s):" op="LOOKUP" value="TC2_CC" extraValue="T. gondii"/>
  </query>
</template>
<template name="Gene_SubmissionSubmission_overlappingBindingSites" title="Gene(s) + Submission(s) + submission(s) --&gt; overlapping binding sites" longDescription="For two submissions or lists of submissions show overlapping binding sites including any genes.  Note:  overlapping does not mean a 100% overlap, the overlap can be complete or partial at either end." comment="">
  <query name="" model="genomic" view="GeneFlankingRegion.chromosome.primaryIdentifier GeneFlankingRegion.chromosomeLocation.end GeneFlankingRegion.chromosomeLocation.start GeneFlankingRegion.overlappingFeatures.submissions.TCid GeneFlankingRegion.overlappingFeatures.overlappingFeatures.submissions.TCid GeneFlankingRegion.gene.primaryIdentifier" longDescription="For two submissions or lists of submissions show overlapping binding sites including any genes.  Note:  overlapping does not mean a 100% overlap, the overlap can be complete or partial at either end." sortOrder="GeneFlankingRegion.chromosome.primaryIdentifier asc" constraintLogic="C and D and A and B and E">
	<constraint path="GeneFlankingRegion.overlappingFeatures.submissions" code="C" editable="true" op="LOOKUP" value="TC2_CC" extraValue="T. gondii"/>
    <constraint path="GeneFlankingRegion.overlappingFeatures.overlappingFeatures.submissions" editable="true" code="D" op="LOOKUP" value="TC1_CS" extraValue="T. gondii"/>
    <constraint path="GeneFlankingRegion.direction" code="A" editable="true" op="=" value="upstream"/>
    <constraint path="GeneFlankingRegion.distance" code="B" editable="true" op="=" value="0.5kb"/>
	<constraint path="GeneFlankingRegion.gene.primaryIdentifier" code="E" editable="true" op="=" value="TGME49_215990"/>
  </query>
</template>
</template-queries>
