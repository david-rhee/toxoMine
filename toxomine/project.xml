<project type="bio">
  <property name="target.model" value="genomic"/>
  <property name="source.location" location="../bio/sources/"/>
  <property name="common.os.prefix" value="common"/>
  <property name="intermine.properties.file" value="toxomine.properties"/>
  <property name="default.intermine.properties.file" location="../default.intermine.integrate.properties"/>
  <sources>
  	<source name="uniprot-toxoplasma" type="uniprot">
      <property name="uniprot.organisms" value="5811"/>
      <property name="createinterpro" value="true"/>
      <property name="creatego" value="true"/>
      <property name="src.data.dir" location="/home/vmuser/toxoDatasources/uniprot/"/>
    </source>
  	<source name="interpro-toxoplasma" type="interpro">
  	  <property name="src.data.dir" location="/home/vmuser/toxoDatasources/interpro"/>
  	</source>
  	<source name="psi-intact" type="psi" dump="true">
  	  <property name="src.data.dir" location="/home/vmuser/toxoDatasources/psi/intact"/>
  	  <property name="intact.organisms" value="5811"/>
  	</source>
  	<source name="psi-mi-ontology" type="psi-mi-ontology">
  	  <property name="src.data.file" location="/home/vmuser/toxoDatasources/psi/obo/psi-mi25.obo"/>
  	</source>
    <source name="gff-toxoplasma" type="toxoDBGFF">
      <property name="gff3.taxonId" value="5811"/>
      <property name="gff3.seqDataSourceName" value="ToxoDB"/>
      <property name="gff3.dataSourceName" value="ToxoDB"/>
      <property name="gff3.seqClsName" value="Chromosome"/>
      <property name="gff3.dataSetTitle" value="ToxoDB T. Gondii ME49 strain Genome Annotation"/>
      <property name="src.data.dir" location="/home/vmuser/toxoDatasources/genome/gff/"/>
    </source>
  	<source name="fasta-toxoplasma" type="fasta" dump="true">
      <property name="fasta.className" value="org.intermine.model.bio.Chromosome"/>
      <property name="fasta.dataSourceName" value="ToxoDB"/>
      <property name="fasta.dataSetTitle" value="ToxoDB T. Gondii ME49 Chromosome Sequence"/>
      <property name="fasta.taxonId" value="5811"/>
      <property name="fasta.includes" value="TGME49_chr*.fasta"/>
      <property name="src.data.dir" location="/home/vmuser/toxoDatasources/genome/fasta/"/>
    </source>
    <!--
    <source name="toxocore-toxoplasma" type="toxoCoreDB">
        <property name="source.db.name" value="toxocore"/>
        <property name="dataSourceName" value="toxoCore"/>
        <property name="dataSetTitle" value="toxoCore Database"/>
        <property name="converter.class" value="org.intermine.bio.dataconversion.ToxoCoreDBConverter"/>
    </source>
    -->
    <source name="toxocore-items-xml" type="toxoCoreItems">
                <property name="src.data.file" location="/home/vmuser/toxoDatasources/toxocore/items/toxocore-items.xml"/>
    </source>
  	<source name="gff-toxoCore" type="toxoCoreGFF">
      <property name="gff3.taxonId" value="5811"/>
      <property name="gff3.seqDataSourceName" value="ToxoDB"/>
      <property name="gff3.dataSourceName" value="ToxoCore"/>
      <property name="gff3.seqClsName" value="Chromosome"/>
      <property name="gff3.dataSetTitle" value="ToxoDB T. Gondii ME49 strain Histone BindingSite"/>
      <property name="src.data.dir" location="/home/vmuser/toxoDatasources/toxocore/gff/"/>
    </source>
 
    <source name="entrez-organism" type="entrez-organism">
      <property name="src.data.file" location="build/organisms.xml"/>
    </source>
  	<source name="go" type="go">
      <property name="src.data.file" location="/home/vmuser/toxoDatasources/go/gene_ontology.obo"/>
      <property name="createrelations" value="false"/>
    </source>
    <source name="pubmed-gene" type="pubmed-gene">
  	  <property name="src.data.dir" location="/home/vmuser/toxoDatasources/pubmed" />
  	  <property name="pubmed.organisms" value="5811"/>
  	  <property name="src.data.dir.includes" value="gene2pubmed"/>
  	  <property name="infoFile" location="/home/vmuser/toxoDatasources/pubmed/gene_info"/>
    </source>
  	<source name="update-publications" type="update-publications" dump="true">
      <!-- this source finds the publication ids (pubmed ids) in the
                 objectstore, retrieves the details of the publication from Entrez
                 into an InterMine format XML file, loads that file into a target
                 items DB, then into the final objectstore dump="true"-->
      <property name="src.data.file" location="build/publications.xml"/>
      <property name="cacheDirName" value="build/"/>
      <property name="loadFullRecord" value="true"/>
    </source>
  </sources>

  <post-processing>
    <post-process name="create-references" />
    <post-process name="set-missing-chromosome-locations"/>
    <post-process name="create-intergenic-region-features"/>
    <post-process name="create-gene-flanking-features" />
  	<!-- <post-process name="transfer-sequences" dump="true"/> -->
    <post-process name="transfer-sequences" />

    <post-process name="do-sources"/>

    <post-process name="create-bioseg-location-index"/>
    <post-process name="create-overlap-view"/>
    
    <!-- webapp -->
  	<!-- <post-process name="create-attribute-indexes" dump="true"/> -->
    <post-process name="create-attribute-indexes" />
    <post-process name="summarise-objectstore"/>
    <post-process name="create-autocomplete-index"/>
    <post-process name="create-search-index"/>
  </post-processing>

</project>

